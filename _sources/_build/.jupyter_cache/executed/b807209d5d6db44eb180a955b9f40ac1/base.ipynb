    "syms t y(t) \n",
    "u_0(t) = heaviside(t);\n",
    "y(t) = exp(-(t-1))*u_0(t-1) - exp(-(t-3))*u_0(t-3);\n",
    "fplot(y(t),[0, 6]),ylim([-1,1.25]),grid,xlabel('t'),ylabel('y(t)')"
   ]
